{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "bundler",
    "strict": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules",
    "prisma",
    "generated"
  ]
}
